{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "API Backend: Flask",
      "type": "debugpy",
      "request": "launch",
      "module": "flask",
      "env": {
        "FLASK_APP": "${workspaceFolder}/backend/flaskr",
        "FLASK_ENV": "development"
      },
      "args": [
        "run",
        "--reload"
      ],
      "jinja": true
    },
    {
      "name": "API Frontend [NodeJs]",
      "type": "node",
      "request": "launch",
      "cwd": "${workspaceFolder}/frontend",
      "runtimeExecutable": "npm",
      "runtimeArgs": [
        "run-script",
        "start"
      ],
      "env": {
        
      }
    },
  ]
}